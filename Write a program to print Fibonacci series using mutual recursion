# Write a program to print Fibonacci series using mutual recursion

def fibonacci(n):
    # Base case
    if n < 1:
        return -1
    elif n == 1:
        return 0
    elif n == 2:
        return 1
    # Recursive case
    else:
        return fibonacci(n - 1) + fibonacci(n - 2)

num = int(input("Enter a number: "))
print("Fibonacci number:", fibonacci(num))
